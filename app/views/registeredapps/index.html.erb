<h1>My Registered Apps</h1>
 <div class="row">
	<div class="col-md-8">
		<% if current_user.registeredapps.any? %>
			<% @registeredapps.each do |app| %>
				<div class="media">
					<div class="media-body">
						<h4 class="media-heading">
							<%= link_to app.name, app %>
						</h4>
					</div>
				</div>
			<% end %>
		<% else %>
			<p>You haven't registered any apps yet.</p>
		<% end %>
	</div>
	<div class="col-md-4", style="padding-top:10px;">
		<%= link_to "Register a new app", new_registeredapp_path, class: 'btn btn-success' %>
	</div>
</div> 
